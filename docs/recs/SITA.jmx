                    break;
                }

            }
			
            for(int j=0; j&lt;inBoundSegRefsString.length; j++){

                if(inBoundSegRefsString[j].equals(n.getAttribute(&quot;SegmentKey&quot;))){
                    inBoundSegmentList.add(n);
                    outBoundSegmentListNodes += domToXML(n);
                    break;
                }


